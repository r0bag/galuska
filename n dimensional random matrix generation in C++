#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;

int main()
{
   int nrow = 0,ncol = 0, i = 0,j = 0;
   
   
   nrow = 10;
   ncol = 10;
   
   int cMatrix[nrow][ncol];
   
   srand(time(0));
   
   for (i=0; i<nrow; i++)
   {
       for (j=0; i<ncol; j++)
       {
            cMatrix[i][j] = 0;         
           
        }
    }
    
    cout << rand() << endl;
    
    
	for (int i=0; i<nrow; i++)
	{
		for (int j=0; j<ncol; j++)
		{
			cout << cMatrix [i][j] << " " ;
		}
	}
   
   
   return 0;
}
